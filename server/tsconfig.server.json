{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./distribution",
    "strict": true,
    "esModuleInterop": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true
  },
  "extends": "../tsconfig.json",
  "include": ["src/**/**/*"],
  "exclude": [ "node_modules", ".vscode", ".server/distribution"],

}