{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./distribution/",

    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true
  },
  "include": ["src/**/**/*"],
  "exclude": [ "node_modules", ".vscode"]
}