{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./distribution/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "composite": true,
    "target": "es5"
  },

  
  "include": [
    "./src/**/**/*"
  ],
  "exclude": ["node_modules"]
}