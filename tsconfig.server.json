{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./distribution/",
    "rootDir": "./server",
    "strict": true,
    "esModuleInterop": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true
  },
  "extends": "../tsconfig.json",
  "include": ["server/src/**/**/*"],
  "exclude": [ "node_modules", ".vscode", "./client", ".server/distribution", "./client/distribution"],

}